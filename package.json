{
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run example:dev:3",
    "build": "node scripts/build.js && npm run build-dts",
    "build-dts": "tsc -p tsconfig.build.json && rollup -c rollup.dts.config.js",
    "lint": "eslint --fix --ext .ts,.vue --no-error-on-unmatched-pattern .",
    "release": "standard-version && git push --follow-tags && npm publish",
    "example:dev:2": "npm run use-vue:2 && npm -C examples/cli-vue2 run serve",
    "example:dev:3": "npm run use-vue:3 && npm -C examples/vite-vue3 run dev",
    "use-vue:2": "vue-demi-switch 2 vue2",
    "use-vue:3": "vue-demi-switch 3",
    "test": "tsc -p tsconfig.build.json"
  },
  "dependencies": {
    "resize-detector": "^0.3.0",
    "vue-demi": "^0.14.7"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/composition-api": "^1.7.2",
    "echarts": "^5.5.0",
    "esbuild": "^0.20.1",
    "eslint": "^8.56.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "execa": "^8.0.1",
    "prettier": "^3.2.5",
    "rollup": "^4.12.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-esbuild": "^6.1.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vue": "^3.4.19",
    "vue-tsc": "^1.8.27",
    "vue2": "npm:vue@^2.6.12"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.5",
    "vue": ">= 2.5 < 2.7 || ^3.1.1"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  }
}
